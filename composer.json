{
    "name": "lumphp/laravel-pipeline",
    "description": "The Illuminate Pipeline package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/lumphp/laravel-pipeline/issues",
        "source": "https://github.com/lumphp/laravel-pipeline"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "lumphp/laravel-contracts": "^8.0",
        "lumphp/laravel-support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Pipeline\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
